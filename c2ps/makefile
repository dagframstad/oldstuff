# things
RM = rm -f

# LIBRARY NAMES GIVEN BELOW
LIBRARIES =

# LINKER GIVEN BELOW
LD = /bin/ld

# INCLUDE SEARCH PATH GIVEN BELOW
INCLUDES =

# LINKER FLAGS GIVEN BELOW
LDFLAGS =

# COMPILER FLAGS GIVEN BELOW
CFLAGS = -Dunix
PFLAGS =$(CFLAGS)
FFLAGS =$(CFLAGS)



.SUFFIXES: .uil .uid .c .o .f .for .F

SRCS = c2ps.c

OBJS = c2ps.o

c2ps: $(OBJS)
	$(CC) -o $@ $(LDFLAGS) $(OBJS) $(LIBRARIES)

.c.o:
	$(CC) -c $(CFLAGS) $(INCLUDES) `pwd`/$<

clean:
	$(RM) $(OBJS) core c2ps *~ *.dsp *.dsw *.ilk *.mak *.mdp *.ncb *.o *.pch *.pdb *.plg vc40.* *.opt ncb
	$(RM) -r Debug


lint:
	lint $(INCLUDES) $(SRCS) $(LINTLIBS)


depend:
	@cat < /dev/null > makedep
	@for i in ${SRCS}; do \
		 ($(CC) -M $(INCLUDES) $$i >> makedep); done
	@echo '/^# DO NOT DELETE THIS LINE/+1,$$d' > eddep
	@echo '$$r makedep' >> eddep
	@echo 'w' >> eddep
	@${CP} Makefile Makefile.bak
	@ed - Makefile < eddep
	@$(RM) -f eddep makedep
	@echo '# DO NOT EDIT THIS FILE HERE.' >> Makefile
	@echo '# USER EDITS MUST PRECEDE THE COMMENT:' >> Makefile
	@echo '# "# DO NOT DELETE THIS LINE".' >> Makefile
	@echo '# see make depend above' >> Makefile


# DO NOT DELETE THIS LINE -- make depend uses it

